
public class Count {
	int number;
	
	public Count() {
	this.number = number;
	}
	
	public int countReverse(int n){
		if(number == n--){
			number = n;
			System.out.println("Numero");
			
		}
		return n;
		
	}
	public static void main(String[] args) {
		Count k = new Count();
		k.countReverse(1);

	}

}
