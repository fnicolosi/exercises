public abstract class Vehicle {
	int wheels;

	public void run(String running) {
	}

}
class Car extends Vehicle {
	int wheels = 4;

	public void run(int a) {
		a = wheels;
		String running="velocità MAX: 300km/H" + a;
		//System.out.println("velocità MAX: 300km/H" + a);
	}

	
}

class Moto extends Vehicle {
	int wheels = 2;

	public void run(int b) {
		b = wheels;
		String running="velocità MAX: 200km/H" + b;
		//System.out.println("velocità MAX: 200km/H" + b);
	}
 


 public static void main(String[] args) {
	 Car One = new Car();
	 One.run(null);
	 
 }
}
