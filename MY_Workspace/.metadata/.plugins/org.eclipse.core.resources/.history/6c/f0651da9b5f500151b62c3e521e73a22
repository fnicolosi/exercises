public class Chess {
	
	public static void main(String[] args) {
		char[][] chessArray = new char[8][8];
		

		for (int i = 0 ; i < 8 ; i++)  
			{ 
			if(i<2){
				System.out.print(" * ");
			}
			
				for (int j = 0; j < 8; j++) {
					chessArray[i][j] = 0;
					if(j<2){
						System.out.print(" * ");
					}
					
				}
								
			}

		}
		
	
}

