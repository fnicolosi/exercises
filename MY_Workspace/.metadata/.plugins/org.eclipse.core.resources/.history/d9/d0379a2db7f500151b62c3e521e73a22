public class Chess {

	public static void main(String[] args) {
		char[][] chessArray = new char[8][8];

		for (int i = 0; i < 8; i++) {
			for (int j = 0; j < 8; j++) {
				// chessArray[i][j] = ;
				if (i < 2 || j < 8) {
					System.out.print(" * ");
				}
				if (i > 2 || i < 5) {
					System.out.print(" _ ");

				}
			}
		}

	}

}
