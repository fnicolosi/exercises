public interface Documenti {
	public boolean uguale(int X);
}

class Libro {
	protected String soggetto;
	protected int costo;

	public Libro(String string, int i) {
	}

	public boolean uguale(String x) {
		
		if (x==this.soggetto) {
			return true;
	} if (this.soggetto.equals(x)) {
			return false;
	}
		return false;
	}

}